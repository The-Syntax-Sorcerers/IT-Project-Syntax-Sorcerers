{
  "buildCommand": "npm run build --prefix client",
  "outputDirectory": "client/dist",
  "devCommand": "npm run dev --prefix client",
  "installCommand": "npm install --prefix client",
  "builds": [
    {
      "src": "server/__init__.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 120
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/__init__.py"
    }
  ]
}
